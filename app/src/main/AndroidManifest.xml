<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asper.karwaan">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-feature android:name="android.hardware.type.watch" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Karwaan">

        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->
        <meta-data
            android:name="com.google.android.wearable.standalone"
            android:value="true" />


        <activity
            android:name=".Nav_activity"
            android:label="@string/title_activity_nav_activity"
            android:theme="@style/Theme.Karwaan.NoActionBar" />

        <activity
            android:name=".Team_activity"
            android:label="Team_avtivity"
            android:theme="@style/Theme.Karwaan.NoActionBar" />

        <activity
            android:name=".About_activity"
            android:label="About_avtivity"
            android:theme="@style/Theme.Karwaan.NoActionBar" />

        <activity
            android:name=".Contact_activity"
            android:label="Contact_avtivity"
            android:theme="@style/Theme.Karwaan.NoActionBar" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>